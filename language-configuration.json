{
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["<", ">"],
        ["'", "'"],
        ["-", ":"],
        ["~", "~"]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "'",
            "close": "'"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "<",
            "close": ">"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "-",
            "close": ":"
        },
        {
            "open": "~",
            "close": "~"
        }
    ],
    "surroundingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "<",
            "close": ">"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "'",
            "close": "'"
        },
        {
            "open": "-",
            "close": ":"
        },
        {
            "open": "~",
            "close": "~"
        }
    ],
    "folding": {
        "markers": {
            "start": "^\\s*<!--\\s*#region\\b(?:\\s*\\[.*\\])?\\s*-->",
            "end": "^\\s*<!--\\s*#endregion\\b(?:\\s*\\[.*\\])?\\s*-->"
        }
    }
}
