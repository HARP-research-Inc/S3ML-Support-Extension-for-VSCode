{
  "label": "S3ML Color Theme",
  "type": "dark",
  "colors": {
    "editor.foreground": "#D4D4D4",
    "editor.background": "#1E1E1E",
    "editor.selectionBackground": "#ADD6FF",
    "editor.lineHighlightBackground": "#FFFFFF08",
    "editorCursor.foreground": "#FFFFFF",
    "editorWhitespace.foreground": "#404040"
  },
  "tokenColors": [
    {
      "scope": "source.s3ml entity.name.tag.s3ml",
      "settings": {
        "foreground": "#67cef3"
      }
    },
    {
      "scope": "source.s3ml entity.name.rename.s3ml",
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "scope": "source.s3ml punctuation.definition.tag.s3ml",
      "settings": {
        "foreground": "#d29797"
      }
    },
    {
      "scope": "source.s3ml entity.name.morphism.s3ml",
      "settings": {
        "foreground": "#1277c4"
      }
    },
    {
      "scope": "source.s3ml punctuation.definition.morphism.s3ml",
      "settings": {
        "foreground": "#fbff00"
      }
    },
    {
      "scope": "source.s3ml entity.name.description.s3ml",
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "scope": "source.s3ml punctuation.definition.description.s3ml",
      "settings": {
        "foreground": "#ebd29f"
      }
    },
    {
      "scope": "source.s3ml entity.name.codeblock.s3ml",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": "source.s3ml punctuation.definition.codeblock.s3ml",
      "settings": {
        "foreground": "#c5c5c5"
      }
    }
  ]
}
